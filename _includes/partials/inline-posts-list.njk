<ul reversed class="blog-posts">
  {% for post in postslist | reverse %} {{post.keys}}
  <li class="{% if post.url == url %} postlist-item-active{% endif %}">
    <time datetime="{{ post.date | readableDate }}"
      >{{ post.date | readableDate }}</time
    >
    &nbsp;-&nbsp;
    <a href="{{ post.url }}" class="postlist-link">
      {% if post.data.title %}{{ post.data.title }}{% else %}<code
        >{{ post.url }}</code
      >{% endif %}
    </a>
  </li>

  {% endfor %}
</ul>
